{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "$id": "https://myrapidi.github.io/jsonschemas/workday_content_all#",
  "title": "Workday RaaS Content (Aligned to Endpoint Names)",
  "description": "Content schema to read Workday report rows (RaaS). Each top-level definition matches its Workday endpoint name exactly.",
  "type": ["object"],
  "definitions": {
    "context": {
      "$schema": "http://json-schema.org/draft-04/hyper-schema",
      "title": "Connection Context",
      "description": "Connection variables for Workday RaaS (tenant alias and report path).",
      "stability": "beta",
      "strictProperties": true,
      "type": ["object"],
      "definitions": {
        "tenant": {
          "description": "Workday tenant alias used in the RaaS path.",
          "example": "standardbiotools_preview",
          "readOnly": true,
          "type": ["string"]
        },
        "report_path": {
          "description": "Workday report path in Workday (folder/name).",
          "example": "ISU_mulesoft/D365_Positions_Report",
          "readOnly": true,
          "type": ["string"]
        }
      },
      "properties": {
        "tenant": { "$ref": "#/definitions/context/definitions/tenant" },
        "report_path": { "$ref": "#/definitions/context/definitions/report_path" }
      }
    },

    "D365_Positions_Report": {
      "$schema": "http://json-schema.org/draft-04/hyper-schema",
      "title": "D365_Positions_Report",
      "description": "A single row from the Workday Positions report.",
      "stability": "beta",
      "strictProperties": true,
      "type": ["object"],
      "definitions": {
        "Full_Name": { "description": "Worker's full name.", "example": "Alicia GÃ³mez", "readOnly": true, "type": ["string"] },
        "Management_Level": { "description": "Management level per Workday.", "example": "Manager", "readOnly": true, "type": ["string"] },
        "Effect_Date_for_Current_Pos": { "description": "Position effective date (YYYY-MM-DD).", "example": "2025-07-01", "readOnly": true, "format": "date", "type": ["string"] },
        "Position_ID": { "description": "Unique identifier of the position.", "example": "POS-00012345", "readOnly": true, "type": ["string"] },
        "Most_Rec_Pos_Change": { "description": "Most recent position change date (YYYY-MM-DD).", "example": "2025-08-15", "readOnly": true, "format": "date", "type": ["string"] },
        "Business_Title": { "description": "Worker's business title.", "example": "Senior Project Manager", "readOnly": true, "type": ["string"] }
      },
      "properties": {
        "Full_Name": { "$ref": "#/definitions/D365_Positions_Report/definitions/Full_Name" },
        "Management_Level": { "$ref": "#/definitions/D365_Positions_Report/definitions/Management_Level" },
        "Effect_Date_for_Current_Pos": { "$ref": "#/definitions/D365_Positions_Report/definitions/Effect_Date_for_Current_Pos" },
        "Position_ID": { "$ref": "#/definitions/D365_Positions_Report/definitions/Position_ID" },
        "Most_Rec_Pos_Change": { "$ref": "#/definitions/D365_Positions_Report/definitions/Most_Rec_Pos_Change" },
        "Business_Title": { "$ref": "#/definitions/D365_Positions_Report/definitions/Business_Title" }
      },
      "links": [
        {
          "title": "Info",
          "description": "Fetch report metadata.",
          "href": "/ccx/service/customreport2/standardbiotools_preview/ISU_mulesoft/D365_Positions_Report?format=json",
          "method": "GET",
          "rel": "self",
          "targetSchema": { "$ref": "#/definitions/D365_Positions_Report" }
        },
        {
          "title": "List",
          "description": "List existing positions.",
          "href": "/ccx/service/customreport2/standardbiotools_preview/ISU_mulesoft/D365_Positions_Report?format=json",
          "method": "GET",
          "rel": "instances",
          "schema": {
            "type": ["object"],
            "properties": {
              "Position_ID": { "description": "Filter: Position ID (exact match).", "example": "POS-250103", "type": ["string"] }
            }
          },
          "targetSchema": { "type": ["array"], "items": { "$ref": "#/definitions/D365_Positions_Report" } }
        }
      ]
    },

