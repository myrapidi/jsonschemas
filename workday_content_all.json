          "href": "/ccx/service/customreport2/standardbiotools_preview/ISU_mulesoft/D365_Employee?format=json",
          "method": "GET",
          "rel": "self",
          "targetSchema": { "$ref": "#/definitions/D365_Employee" }
        },
        {
          "title": "List",
          "description": "List employees.",
          "href": "/ccx/service/customreport2/standardbiotools_preview/ISU_mulesoft/D365_Employee?format=json",
          "method": "GET",
          "rel": "instances",
          "schema": {
            "type": ["object"],
            "properties": {
              "Position_ID": { "description": "Optional filter.", "type": ["string"] },
              "Employee_ID": { "type": ["integer"] }
            }
          },
          "targetSchema": { "type": ["array"], "items": { "$ref": "#/definitions/D365_Employee" } }
        }
      ]
