          "type": [
            "string"
          ]
        },
        "Effective_Date_for_Current_Position": {
          "description": "Position effective date (YYYY-MM-DD).",
          "example": "2025-07-01",
          "readOnly": true,
          "format": "date",
          "type": [
            "string"
          ]
        },
        "Position_ID": {
          "description": "Unique identifier of the position.",
          "example": "POS-00012345",
          "readOnly": true,
          "type": [
            "string"
          ]
        },
        "Most_Rec_Pos_Change": {
          "description": "Most recent position change date (YYYY-MM-DD).",
          "example": "2025-08-15",
          "readOnly": true,
          "format": "date",
          "type": [
            "string"
          ]
        },
        "Most_Recent_Position_Change_start": {
          "description": "Most recent position change date (YYYY-MM-DD).",
          "example": "2025-08-15",
          "readOnly": true,
          "format": "date",
          "type": [
            "string"
          ]
        },
        "Position": {
          "description": "Position description.",
          "example": "P100006 Vice President, Technology Research",
          "readOnly": true,
          "type": [
            "string"
          ]
        },
        "Active_Status": {
          "title": "Active_Status",
          "type": [
            "boolean"
          ]
        },
        "Business_Title": {
          "description": "Worker's business title.",
          "example": "Senior Project Manager",
          "readOnly": true,
          "type": [
            "string"
          ]
        }
