{
  "title": "Workday RaaS (Multi-Report)",
  "description": "Schema covering Employee, Position Worker Assignment, and Position Hierarchies. RaaS is read-only (GET). POST/PATCH/DELETE links are illustrative only.",
  "type": "object",
  "definitions": {
    "context": {
      "title": "Connection Context",
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "tenant": {
          "type": "string",
          "description": "e.g., standardbiotools_preview"
        },
        "folder": {
          "type": "string",
          "description": "e.g., ISU_mulesoft"
        },
        "Report_Name": {
          "type": "string",
          "enum": [
            "D365_Employee",
            "D365_Position_Worker_Assignment",
            "D365_Position_Hierarchies"
          ],
          "description": "Target report name"
        }
      },
      "required": [
        "tenant",
        "folder",
        "Report_Name"
      ]
    },
    "reports": {
      "D365_Employee": {
        "title": "D365_Employee Row",
        "type": "object",
        "additionalProperties": false,
        "properties": {
          "Active_Status": {
            "type": "string"
          },
          "Company": {
            "type": "string"
          },
          "Cost_Center": {
            "type": "string"
          },
          "Cost_Center_-_ID": {
            "type": "string"
          },
          "Employee_ID": {
            "type": "string"
          },
          "Hire_Date": {
            "type": "string",
            "format": "date"
          },
          "Preferred_Name_-_First_Name": {
            "type": "string"
          },
          "Preferred_Name_-_Last_Name": {
            "type": "string"
          }
        },
        "required": [
          "Employee_ID"
        ]
      },
      "D365_Position_Worker_Assignment": {
        "title": "D365_Position_Worker_Assignment Row",
        "type": "object",
        "additionalProperties": false,
        "properties": {
          "Active_Status": {
            "type": "string"
          },
          "Current_Position_Filled_-_Effective_Date": {
            "type": "string",
            "format": "date"
          },
          "Employee_ID": {
            "type": "string"
          },
          "Last_Functionally_Updated": {
            "type": "string",
            "format": "date-time"
          },
          "Position_ID": {
            "type": "string"
          }
        },
        "required": [
          "Position_ID"
        ]
      },
      "D365_Position_Hierarchies": {
        "title": "D365_Position_Hierarchies Row",
        "type": "object",
        "additionalProperties": false,
        "properties": {
          "Active_Status": {
            "type": "string"
          },
          "Full_Name": {
            "type": "string"
          },
          "HIERARCHYTYPENAME": {
            "type": "string"
          },
          "Manager_s_Position_group": {
            "type": "array",
            "items": {
              "type": "object",
              "additionalProperties": false,
              "properties": {
                "Manager_Most_Rec_POS_Change": {
                  "type": "string",
                  "format": "date"
                },
                "Manager_POS_ID": {
                  "type": "string"
                }
              }
            }
          },
          "Position_ID": {
            "type": "string"
          },
          "VALIDFROM": {
            "type": "string"
          },
          "VALIDTO": {
            "type": "string"
          }
        },
        "required": [
          "Position_ID"
        ]
      }
    },
    "report": {
      "title": "Workday RaaS Row (Any supported report)",
      "oneOf": [
        {
          "$ref": "#/definitions/reports/D365_Employee"
        },
        {
          "$ref": "#/definitions/reports/D365_Position_Worker_Assignment"
        },
        {
          "$ref": "#/definitions/reports/D365_Position_Hierarchies"
        }
      ]
    }
  },
  "properties": {
    "context": {
      "$ref": "#/definitions/context"
    },
    "Report_Entry": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/report"
      }
    }
  },
  "required": [
    "context",
    "Report_Entry"
  ],
  "links": [
    {
      "title": "Create (generic)",
      "description": "Illustrative only; RaaS is read-only.",
      "href": "/ccx/service/customreport2/{tenant}/{folder}/{Report_Name}?format=json",
      "method": "POST",
      "rel": "create",
      "schema": {
        "type": [
          "object"
        ]
      }
    },
    {
      "title": "Update (generic)",
      "description": "Illustrative only; RaaS is read-only.",
      "href": "/ccx/service/customreport2/{tenant}/{folder}/{Report_Name}?format=json",
      "method": "PATCH",
