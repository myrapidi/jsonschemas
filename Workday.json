{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "id": "https://myrapidi.github.com/jsonschemas/Workday_SB_Schema_v2#",
  "title": "Workday – D365 Positions (RaaS) – Schema v2 (inline Report_Entry)",
  "description": "Root has 'report' (metadata) and 'Report_Entry' (array of rows). No $ref for row fields.",
  "type": "object",
  "properties": {
    "report": {
      "title": "report",
      "description": "Workday RaaS report metadata (maps to Storyblok 'space').",
      "type": "object",
      "properties": {
        "report_name": {
          "type": "string",
          "description": "Path/name of the Workday report (folder/name).",
          "example": "ISU_mulesoft/D365_Positions_Report",
          "readOnly": true
        },
        "endpoint": {
          "type": "string",
          "description": "Fully qualified RaaS endpoint URL.",
          "example": "https://wd5-impl-services1.workday.com/ccx/service/customreport2/standardbiotools_preview/ISU_mulesoft/D365_Positions_Report?format=json",
          "readOnly": true
        }
      },
      "additionalProperties": true
    },
    "Report_Entry": {
      "title": "Report_Entry",
      "description": "Array of Workday RaaS rows.",
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "Active_Status": {
            "type": "boolean",
            "description": "Active status of the position."
          },
          "Business_Title": {
            "type": "string",
            "description": "Business title of the worker."
          },
          "Effect_Date_for_Current_Pos": {
            "type": "string",
            "format": "date-time",
            "description": "Effective date for the current position."
          },
          "Full_Name": {
            "type": "string",
            "description": "Full name of the worker."
          },
          "Management_Level": {
            "type": "string",
            "description": "Worker's management level."
          },
          "Most_Rec_Pos_Change": {
            "type": "string",
            "format": "date-time",
            "description": "Most recent position change date."
          },
          "Position_ID": {
            "type": "string",
            "description": "Workday Position ID."
          }
        },
        "additionalProperties": true
      }
    }
  },
  "required": [
    "Report_Entry"
  ],
  "additionalProperties": true
}
