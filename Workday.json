{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "id": "https://myrapidi.github.com/jsonschemas/workday_raas_–_positions_metadata",
  "title": "Workday RaaS – Positions Metadata",
  "description": "Schema for metadata extraction via RaaS",
  "type": [
    "object"
  ],
  "definitions": {
    "position": {
      "description": "Workday RaaS Position Report Entry.",
      "$schema": "http://json-schema.org/draft-04/hyper-schema",
      "stability": "beta",
      "strictProperties": true,
      "title": "Position Entry",
      "type": [
        "object"
      ],
      "definitions": {
        "Full_Name": {
          "description": "Worker's full name.",
          "example": "Alicia Gómez",
          "readOnly": true,
          "type": [
            "string"
          ]
        },
        "Management_Level": {
          "description": "Management level per Workday.",
          "example": "Manager",
          "readOnly": true,
          "type": [
            "string"
          ]
        },
        "Effect_Date_for_Current_Pos": {
          "description": "Position effective date (YYYY-MM-DD).",
          "example": "2025-07-01",
          "readOnly": true,
          "type": "string",
          "format": "date"
        },
        "Position_ID": {
          "description": "Unique identifier of the position.",
          "example": "POS-00012345",
          "readOnly": true,
          "type": [
            "string"
          ]
        },
        "Most_Rec_Pos_Change": {
          "description": "Most recent position change date (YYYY-MM-DD).",
          "example": "2025-08-15",
          "readOnly": true,
          "type": "string",
          "format": "date"
        },
        "Business_Title": {
          "description": "Worker's business title.",
          "example": "Senior Project Manager",
          "readOnly": true,
          "type": [
            "string"
          ]
        }
      },
      "properties": {
        "Full_Name": {
          "$ref": "#/definitions/position/definitions/Full_Name"
        },
        "Management_Level": {
          "$ref": "#/definitions/position/definitions/Management_Level"
        },
        "Effect_Date_for_Current_Pos": {
          "$ref": "#/definitions/position/definitions/Effect_Date_for_Current_Pos"
        },
        "Position_ID": {
          "$ref": "#/definitions/position/definitions/Position_ID"
        },
        "Most_Rec_Pos_Change": {
          "$ref": "#/definitions/position/definitions/Most_Rec_Pos_Change"
        },
        "Business_Title": {
          "$ref": "#/definitions/position/definitions/Business_Title"
        }
      }
    }
  },
  "properties": {
    "Report_Entry": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/position"
      }
    }
  },
  "links": [
    {
      "rel": "self",
      "description": "Fetch full Workday report (unfiltered).",
      "href": "https://wd5-impl-services1.workday.com/ccx/service/customreport2/standardbiotools_preview/ISU_mulesoft/WD_D365_Positions_Report?format=json",
      "method": "GET"
    },
    {
      "rel": "instances",
      "description": "Fetch filtered report using Position_ID prompt.",
      "href": "https://wd5-impl-services1.workday.com/ccx/service/customreport2/standardbiotools_preview/ISU_mulesoft/WD_D365_Positions_Report?Position_ID={Position_ID}&format=json",
      "method": "GET",
      "schema": {
        "type": "object",
        "properties": {
          "Position_ID": {
            "type": "string",
            "description": "Workday report prompt key: Position_ID",
            "example": "POS-133"
          }
        },
        "required": [
          "Position_ID"
        ]
      }
    }
  ]
}
