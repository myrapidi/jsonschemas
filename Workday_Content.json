{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "type": ["object"],
  "definitions": {
    "report": {
      "description": "Rows for the Workday Positions report.",
      "$schema": "http://json-schema.org/draft-04/hyper-schema",
      "stability": "beta",
      "strictProperties": true,
      "title": "Positions",
      "type": ["object"],
      "definitions": {
        "Full_Name": {
          "description": "Worker's full name.",
          "example": "Alicia Gómez",
          "readOnly": true,
          "type": ["string"]
        },
        "Management_Level": {
          "description": "Management level per Workday.",
          "example": "Manager",
          "readOnly": true,
          "type": ["string"]
        },
        "POS_Effect_Date": {
          "description": "Position effective date (YYYY-MM-DD).",
          "example": "2025-07-01",
          "readOnly": true,
          "type": ["string"]
        },
        "Position_ID": {
          "description": "Unique identifier of the position.",
          "example": "P-00012345",
          "readOnly": true,
          "type": ["string"]
        },
        "Rec_POS_Change": {
          "description": "Most recent position change date (YYYY-MM-DD).",
          "example": "2025-08-15",
          "readOnly": true,
          "type": ["string"]
        },
        "business_Title": {
          "description": "Worker's business title.",
          "example": "Senior Project Manager",
          "readOnly": true,
          "type": ["string"]
        }
      },
      "links": [
        {
          "description": "Fetch metadata about the report.",
          "href": "/ccx/service/customreport2/{tenant}/{report_path}",
          "method": "GET",
          "rel": "self",
          "targetSchema": { "$ref": "#/definitions/report" },
          "title": "Report Metadata"
        },
        {
          "description": "List rows from the Workday report.",
          "href": "/ccx/service/customreport2/{tenant}/{report_path}?format=json",
          "method": "GET",
          "rel": "instances",
          "targetSchema": {
            "items": { "$ref": "#/definitions/report" },
            "type": ["array"]
          },
          "title": "Report Rows"
        }
      ],
      "properties": {
        "Full_Name": { "$ref": "#/definitions/report/definitions/Full_Name" },
        "Management_Level": { "$ref": "#/definitions/report/definitions/Management_Level" },
        "POS_Effect_Date": { "$ref": "#/definitions/report/definitions/POS_Effect_Date" },
        "Position_ID": { "$ref": "#/definitions/report/definitions/Position_ID" },
        "Rec_POS_Change": { "$ref": "#/definitions/report/definitions/Rec_POS_Change" },
        "business_Title": { "$ref": "#/definitions/report/definitions/business_Title" }
      }
    }
  },
  "properties": {
    "report": {
      "$ref": "#/definitions/report"
    }
  },
  "description": "Workday WD_D365_Positions_Report schema (read-only, fields under report object).",
  "id": "https://myrapidi.github.com/jsonschemas/workday/wd_d365_positions_report_object#",
  "links": [
    {
      "href": "https://wd5-impl-services1.workday.com/ccx/service/customreport2/standardbiotools_preview/ISU_mulesoft/WD_D365_Positions_Report?format=json",
      "rel": "self"
    }
  ],
  "title": "Workday – WD_D365_Positions_Report (report object)"
}
