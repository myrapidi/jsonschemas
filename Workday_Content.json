{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "id": "https://myrapidi.github.com/jsonschemas/workday_positions_content#",
  "title": "Workday RaaS Report Content - Position Details",
  "description": "Schema for reading Workday positions content via RaaS (REST API).",
  "type": "object",
  "properties": {
    "Report_Entry": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "Full_Name": {
            "description": "Worker's full name.",
            "example": "Alicia G\u00f3mez",
            "readOnly": true,
            "type": "string"
          },
          "Management_Level": {
            "description": "Management level per Workday.",
            "example": "Manager",
            "readOnly": true,
            "type": "string"
          },
          "Effect_Date_for_Current_Pos": {
            "description": "Position effective date (YYYY-MM-DD).",
            "example": "2025-07-01",
            "readOnly": true,
            "type": "string",
            "format": "date"
          },
          "Position_ID": {
            "description": "Unique identifier of the position.",
            "example": "POS-00012345",
            "readOnly": true,
            "type": "string"
          },
          "Most_Rec_Pos_Change": {
            "description": "Most recent position change date (YYYY-MM-DD).",
            "example": "2025-08-15",
            "readOnly": true,
            "type": "string",
            "format": "date"
          },
          "Business_Title": {
            "description": "Worker's business title.",
            "example": "Senior Project Manager",
            "readOnly": true,
            "type": "string"
          }
        },
        "required": [
          "Position_ID",
          "Most_Rec_Pos_Change"
        ]
      }
    }
  }
}
