{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "$id": "urn:schema:workday:wd_d365_positions_report:v1",
  "title": "Workday WD_D365_Positions_Report",
  "description": "Schema for the Workday Custom Report JSON response (customreport2) for WD_D365_Positions_Report.",
  "type": "object",
  "definitions": {
    "service": {
      "stability": "beta",
      "strictProperties": true,
      "title": "Workday Custom Report Service - WD_D365_Positions_Report",
      "type": "object",
      "definitions": {
        "reportEntry": {
          "description": "Single row from the Workday WD_D365_Positions_Report.",
          "type": "object",
          "additionalProperties": false,
          "properties": {
            "Full_Name": {
              "description": "Worker full name",
              "type": "string",
              "maxLength": 200
            },
            "Management_Level": {
              "description": "Management level as configured in Workday",
              "type": "string",
              "maxLength": 100
            },
            "POS_Effect_Date": {
              "description": "Position effective date (ISO, YYYY-MM-DD)",
              "type": "string",
              "pattern": "^\\d{4}-\\d{2}-\\d{2}$"
            },
            "Position_ID": {
              "description": "Workday Position ID",
              "type": "string",
              "maxLength": 100
            },
            "Rec_POS_Change": {
              "description": "Recent position change date (ISO, YYYY-MM-DD)",
              "type": "string",
              "pattern": "^\\d{4}-\\d{2}-\\d{2}$"
            },
            "business_Title": {
              "description": "Business title from Workday",
              "type": "string",
              "maxLength": 200
            }
          },
          "required": [
            "Full_Name",
            "Management_Level",
            "POS_Effect_Date",
            "Position_ID",
            "Rec_POS_Change",
            "business_Title"
          ]
        },
        "reportEntryArray": {
          "description": "Collection of report rows",
          "type": "array",
          "items": { "$ref": "#/definitions/service/definitions/reportEntry" }
        }
      },
      "properties": {
        "Report_Entry": {
          "description": "Top-level array returned by Workday Custom Report.",
          "$ref": "#/definitions/service/definitions/reportEntryArray"
        }
      },
      "required": ["Report_Entry"],
      "additionalProperties": false,
      "example": {
        "Report_Entry": [
          {
            "Full_Name": "John Smith1",
            "Management_Level": "Individual Contributor",
            "POS_Effect_Date": "2024-08-09",
            "Position_ID": "PO-000",
            "Rec_POS_Change": "2024-08-09",
            "business_Title": "Director of OM"
          }
        ]
      }
    }
  },
  "properties": {
    "Report_Entry": {
      "$ref": "#/definitions/service/properties/Report_Entry"
    }
  },
  "required": ["Report_Entry"],
  "additionalProperties": false
}
