{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "id": "https://myrapidi.github.com/jsonschemas/wd_raas_positions_content#",
  "title": "Workday RaaS – Positions Report (Content Layout)",
  "description": "Schema reflecting a content-style layout for reading Workday RaaS reports.",
  "type": [
    "object"
  ],
  "definitions": {
    "service": {
      "description": "Workday RaaS service for Positions report.",
      "$schema": "http://json-schema.org/draft-04/hyper-schema",
      "stability": "beta",
      "strictProperties": true,
      "title": "Workday RaaS API – Positions",
      "type": [
        "object"
      ],
      "definitions": {
        "service_id": {
          "description": "Identifier for the service definition (static).",
          "example": "wd_raas_positions",
          "readOnly": true,
          "type": [
            "string"
          ]
        },
        "description": {
          "description": "Description of the service.",
          "example": "Read-only access to the Workday RaaS Positions report.",
          "readOnly": true,
          "type": [
            "string"
          ]
        }
      },
      "properties": {
        "service_id": {
          "$ref": "#/definitions/service/definitions/service_id"
        },
        "description": {
          "$ref": "#/definitions/service/definitions/description"
        }
      }
    },
    "report": {
      "description": "Positions report wrapper object.",
      "$schema": "http://json-schema.org/draft-04/hyper-schema",
      "stability": "beta",
      "strictProperties": true,
      "title": "Positions Report",
      "type": [
        "object"
      ],
      "definitions": {
        "Full_Name": {
          "description": "Full name of the worker.",
          "example": "Alicia Gómez",
          "readOnly": false,
          "type": [
            "string"
          ]
        },
        "Management_Level": {
          "description": "Worker's management level.",
          "example": "Manager",
          "readOnly": false,
          "type": [
            "string"
          ]
        },
        "Effect_Date_for_Current_Pos": {
          "description": "Effective date of current position.",
          "example": "2025-07-01",
          "format": "date",
          "readOnly": false,
          "type": [
            "string"
          ]
        },
        "Position_ID": {
          "description": "Position identifier.",
          "example": "POS-00012345",
          "readOnly": false,
          "type": [
            "string"
          ]
        },
        "Most_Rec_Pos_Change": {
          "description": "Date of most recent position change.",
          "example": "2025-08-15",
          "format": "date",
          "readOnly": false,
          "type": [
            "string"
          ]
        },
        "Business_Title": {
          "description": "Worker's business title.",
          "example": "Senior Project Manager",
          "readOnly": false,
          "type": [
            "string"
          ]
        },
        "report_entry": {
          "description": "Single row/entry from the RaaS report.",
          "type": [
            "object"
          ],
          "properties": {
            "Full_Name": {
              "$ref": "#/definitions/report/definitions/Full_Name"
            },
            "Management_Level": {
              "$ref": "#/definitions/report/definitions/Management_Level"
            },
            "Effect_Date_for_Current_Pos": {
              "$ref": "#/definitions/report/definitions/Effect_Date_for_Current_Pos"
            },
            "Position_ID": {
              "$ref": "#/definitions/report/definitions/Position_ID"
            },
            "Most_Rec_Pos_Change": {
              "$ref": "#/definitions/report/definitions/Most_Rec_Pos_Change"
            },
            "Business_Title": {
              "$ref": "#/definitions/report/definitions/Business_Title"
            }
          }
        }
      },
      "properties": {
        "Report_Entry": {
          "description": "Array of report entries returned by the Workday report.",
          "type": [
            "array"
          ],
          "items": {
            "$ref": "#/definitions/report/definitions/report_entry"
          }
        }
      },
      "links": [
        {
          "description": "Fetch the report results (read-only).",
          "href": "https://wd5-impl-services1.workday.com/ccx/service/customreport2/standardbiotools_preview/ISU_mulesoft/WD_D365_Positions_Report?format=json",
          "method": "GET",
          "rel": "self",
          "title": "Fetch"
        }
      ]
    }
  },
  "properties": {
    "service": {
      "$ref": "#/definitions/service"
    },
    "report": {
      "$ref": "#/definitions/report"
    }
  },
  "links": [
    {
      "href": "https://wd5-impl-services1.workday.com/ccx/service/customreport2/standardbiotools_preview/ISU_mulesoft/WD_D365_Positions_Report?format=json",
      "rel": "self"
    }
  ]
}
