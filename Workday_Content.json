{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "id": "https://myrapidi.github.com/jsonschemas/Workday_RaaS_Content_StoryblokStyle#",
  "title": "Workday – D365 Positions (RaaS) – Storyblok-style content mapping",
  "description": "Storyblok content-style mapping for Workday RaaS. In this mapping: 'report' ≈ 'space', and 'story' ≈ 'Report_Entry'.",
  "type": [
    "object"
  ],
  "definitions": {
    "report": {
      "description": "A Workday RaaS report (maps to Storyblok 'space').",
      "$schema": "http://json-schema.org/draft-04/hyper-schema",
      "stability": "beta",
      "strictProperties": true,
      "title": "Workday RaaS Report",
      "type": [
        "object"
      ],
      "definitions": {
        "report_name": {
          "description": "Path/name of the report in Workday (folder/name).",
          "example": "ISU_mulesoft/D365_Positions_Report",
          "readOnly": true,
          "type": [
            "string"
          ]
        },
        "endpoint": {
          "description": "Fully qualified RaaS endpoint URL.",
          "example": "https://wd5-impl-services1.workday.com/ccx/service/customreport2/standardbiotools_preview/ISU_mulesoft/D365_Positions_Report?format=json",
          "readOnly": true,
          "type": [
            "string"
          ]
        }
      },
      "properties": {
        "report_name": {
          "$ref": "#/definitions/report/definitions/report_name"
        },
        "endpoint": {
          "$ref": "#/definitions/report/definitions/endpoint"
        }
      }
    },
    "story": {
      "description": "Single Workday RaaS record (maps to Storyblok 'story').",
      "$schema": "http://json-schema.org/draft-04/hyper-schema",
      "stability": "beta",
      "strictProperties": true,
      "title": "Report_Entry",
      "type": [
        "object"
      ],
      "definitions": {
        "Active_Status": {
          "description": "Active status of the position.",
          "example": true,
          "readOnly": false,
          "type": "Boolean"
        },
        "Business_Title": {
          "description": "Business title of the worker.",
          "example": "Director of Software Operations",
          "readOnly": false,
          "type": "string"
        },
        "Effect_Date_for_Current_Pos": {
          "description": "Effective date for the current position.",
          "example": "2024-08-09",
          "readOnly": false,
          "type": "datetime"
        },
        "Full_Name": {
          "description": "Full name of the worker.",
          "example": "Gregory Harris",
          "readOnly": false,
          "type": "string"
        },
        "Management_Level": {
          "description": "Worker's management level.",
          "example": "Individual Contributor",
          "readOnly": false,
          "type": "string"
        },
        "Most_Rec_Pos_Change": {
          "description": "Most recent position change date.",
          "example": "2024-08-09",
          "readOnly": false,
          "type": "datetime"
        },
        "Position_ID": {
          "description": "Workday Position ID.",
          "example": "POS-133",
          "readOnly": false,
          "type": "string"
        }
      },
      "properties": {
        "Active_Status": {
          "$ref": "#/definitions/story/definitions/Active_Status"
        },
        "Business_Title": {
          "$ref": "#/definitions/story/definitions/Business_Title"
        },
        "Effect_Date_for_Current_Pos": {
          "$ref": "#/definitions/story/definitions/Effect_Date_for_Current_Pos"
        },
        "Full_Name": {
          "$ref": "#/definitions/story/definitions/Full_Name"
        },
        "Management_Level": {
          "$ref": "#/definitions/story/definitions/Management_Level"
        },
        "Most_Rec_Pos_Change": {
          "$ref": "#/definitions/story/definitions/Most_Rec_Pos_Change"
        },
        "Position_ID": {
          "$ref": "#/definitions/story/definitions/Position_ID"
        }
      }
    }
  },
  "properties": {
    "report": {
      "$ref": "#/definitions/report"
    },
    "story": {
      "$ref": "#/definitions/story"
    }
  }
}
