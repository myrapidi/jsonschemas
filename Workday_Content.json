{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "type": [
    "object"
  ],
  "definitions": {
    "report": {
      "description": "Rows for the Workday Positions report.",
      "$schema": "http://json-schema.org/draft-04/hyper-schema",
      "stability": "beta",
      "strictProperties": true,
      "title": "Positions",
      "type": [
        "object"
      ],
      "definitions": {
        "Full_Name": {
          "description": "Worker's full name.",
          "example": "Alicia G\u00f3mez",
          "readOnly": true,
          "type": [
            "string"
          ]
        },
        "Management_Level": {
          "description": "Management level per Workday.",
          "example": "Manager",
          "readOnly": true,
          "type": [
            "string"
          ]
        },
        "Effect_Date_for_Current_Pos": {
          "description": "Position effective date (YYYY-MM-DD).",
          "example": "2025-07-01",
          "readOnly": true,
          "type": [
            "string"
          ]
        },
        "Position_ID": {
          "description": "Unique identifier of the position.",
          "example": "P-00012345",
          "readOnly": true,
          "type": [
            "string"
          ]
        },
        "Most_Rec_Pos_Change": {
          "description": "Most recent position change date (YYYY-MM-DD).",
          "example": "2025-08-15",
          "readOnly": true,
          "type": [
            "string"
          ]
        },
        "Business_Title": {
          "description": "Worker's business title.",
          "example": "Senior Project Manager",
          "readOnly": true,
          "type": [
            "string"
          ]
        }
      },
      "links": [
        {
          "description": "Fetch metadata about the report.",
          "href": "/ccx/service/customreport2/{(%23%2Fdefinitions%2Fspace%2Fdefinitions%2Ftenant)}/{(%23%2Fdefinitions%2Fspace%2Fdefinitions%2Freport_path)}",
          "method": "GET",
          "rel": "self",
          "targetSchema": {
            "$ref": "#/definitions/space"
          },
          "title": "Report Metadata"
        },
        {
          "description": "List rows from the Workday report.",
          "href": "/ccx/service/customreport2/{(%23%2Fdefinitions%2Fspace%2Fdefinitions%2Ftenant)}/{(%23%2Fdefinitions%2Fspace%2Fdefinitions%2Freport_path)}?format=json",
          "method": "GET",
          "rel": "instances",
          "targetSchema": {
            "type": [
              "array"
            ],
            "items": {
              "$ref": "#/definitions/report"
            }
          },
          "title": "Report Rows"
        }
      ],
      "properties": {
        "Full_Name": {
          "$ref": "#/definitions/report/definitions/Full_Name"
        },
        "Management_Level": {
          "$ref": "#/definitions/report/definitions/Management_Level"
        },
        "Effect_Date_for_Current_Pos": {
          "$ref": "#/definitions/report/definitions/Effect_Date_for_Current_Pos"
        },
        "Position_ID": {
          "$ref": "#/definitions/report/definitions/Position_ID"
        },
        "Most_Rec_Pos_Change": {
          "$ref": "#/definitions/report/definitions/Most_Rec_Pos_Change"
        },
        "Business_Title": {
          "$ref": "#/definitions/report/definitions/Business_Title"
        }
      }
    },
    "space": {
      "description": "Connection variables for Workday RaaS (provided by connector).",
      "$schema": "http://json-schema.org/draft-04/hyper-schema",
      "stability": "beta",
      "strictProperties": true,
      "title": "Connection",
      "type": [
        "object"
      ],
      "definitions": {
        "tenant": {
          "description": "Workday tenant alias used in the RaaS path.",
          "example": "standardbiotools_preview",
          "readOnly": true,
          "type": [
            "string"
          ]
        },
        "report_path": {
          "description": "Workday report path 'Folder/Report_Name'.",
          "example": "ISU_mulesoft/WD_D365_Positions_Report",
          "readOnly": true,
          "type": [
            "string"
          ]
        }
      },
      "properties": {
        "tenant": {
          "$ref": "#/definitions/space/definitions/tenant"
        },
        "report_path": {
          "$ref": "#/definitions/space/definitions/report_path"
        }
      }
    }
  },
  "properties": {
    "report": {
      "$ref": "#/definitions/report"
    }
  },
  "description": "Workday WD_D365_Positions_Report schema (read-only, fields under report object).",
  "id": "https://myrapidi.github.com/jsonschemas/workday/wd_d365_positions_report_object#",
  "title": "Workday \u2013 WD_D365_Positions_Report (report object)"
}
